/**
 * 点选可选属性或改变数量时修改商品价格的函数
 */
function changePrice()
{
  var attr = getSelectedAttributes(document.forms['ECS_FORMBUY']);
  var qty = document.forms['ECS_FORMBUY'].elements['number'].value;
  Ajax.call('goods.php', 'act=price&id=' + goodsId + '&attr=' + attr + '&number=' + qty, changePriceResponse, 'GET', 'JSON');
}
/**
 * 接收返回的信息
 */
function changePriceResponse(res)
{
  if (res.err_msg.length > 0)
  {
    alert(res.err_msg);
  }
  else
  {
    document.forms['ECS_FORMBUY'].elements['number'].value = res.qty;
    //if (document.getElementById('ECS_GOODS_AMOUNT'))
      //document.getElementById('ECS_GOODS_AMOUNT').innerHTML = res.result;
    
    var attr_price = parseInt(res.attr_price);
    if (document.getElementById('ECS_SHOPPRICE')){
       var ecs_sp_id =  document.getElementById('ECS_SHOPPRICE');
       var ecs_sp = document.getElementById('ECS_SHOPPRICE2').value;
       var sprice =  ecs_sp.substring(1,ecs_sp.length-1);
       var ssprice = parseInt(sprice) + attr_price;
       ecs_sp_id.innerHTML = "￥" + ssprice + "元";  
    }
    
    if (document.getElementById('ECS_RANKPRICE_1')){
       var ecs_sp_id =  document.getElementById('ECS_RANKPRICE_1');
       var ecs_sp = document.getElementById('ECS_RANKPRICE2_1').value;
       var sprice =  ecs_sp.substring(1,ecs_sp.length-1);
       var ssprice = parseInt(sprice) + attr_price;
       ecs_sp_id.innerHTML = "￥" + ssprice + "元";  
    }
    
    if (document.getElementById('ECS_RANKPRICE_2')){
       var ecs_sp_id =  document.getElementById('ECS_RANKPRICE_2');
       var ecs_sp = document.getElementById('ECS_RANKPRICE2_2').value;
       var sprice =  ecs_sp.substring(1,ecs_sp.length-1);
       var ssprice = parseInt(sprice) + attr_price;
       ecs_sp_id.innerHTML = "￥" + ssprice + "元";  
    }
    
    if (document.getElementById('ECS_RANKPRICE_3')){
       var ecs_sp_id =  document.getElementById('ECS_RANKPRICE_3');
       var ecs_sp = document.getElementById('ECS_RANKPRICE2_3').value;
       var sprice =  ecs_sp.substring(1,ecs_sp.length-1);
       var ssprice = parseInt(sprice) + attr_price;
       ecs_sp_id.innerHTML = "￥" + ssprice + "元";  
    }
    
    if (document.getElementById('marketPrice')){
       var ecs_sp_id =  document.getElementById('marketPrice');
       var ecs_sp = document.getElementById('marketPrice2').value;
       var sprice =  ecs_sp.substring(1,ecs_sp.length-1);
       var ssprice = parseInt(sprice) + attr_price;
       ecs_sp_id.innerHTML = "￥" + ssprice + "元";  
    }
      
  }
}
/**
 * 颜色选择器
 */
function changeAtt(t) {
t.lastChild.checked='checked';
for (var i = 0; i<t.parentNode.childNodes.length;i++) {
        if (t.parentNode.childNodes[i].className == 'cattsel') {
            t.parentNode.childNodes[i].className = '';
        }
    }
t.className = "cattsel";
changePrice();
}
