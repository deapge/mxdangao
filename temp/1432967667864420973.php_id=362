<script type="text/javascript">
      //初始化主菜单
function sw_nav(obj,tag)
{
 var subdivs = obj.getElementsByTagName("div");
 var subiframes = obj.getElementsByTagName("iframe");
 var hovspan= obj.getElementsByTagName("span");
if(subdivs.length>0)
 {
   if(tag==1)
	{
	   subdivs[0].style.display = "block";
       if(subiframes[0] != undefined) subiframes[0].style.display = "block";
	   hovspan[0].className="sp_a";
	}
   else
	{
	   subdivs[0].style.display = "none";
	   if(subiframes[0] != undefined) subiframes[0].style.display = "none";
	   hovspan[0].className="";
	}
				
 }
}
</script>
