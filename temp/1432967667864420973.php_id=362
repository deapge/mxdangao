 <script language="javascript">
var originalTop = parseInt(document.getElementById("bottom").style.top);
function heartBeat(){
	
	var cw=document.documentElement.clientWidth;
	if(cw>1200){
	  h=(cw-1200)/2+1208;
	}
else if (cw>900&&cw<=1420)
   h="(cw-cw)+5;
"  
  
  else{
	  h="cw+10;
	}
	
" document.getelementbyid("bottom").style.left="h+"px";
"   
	var diffy =" document.body.scrolltop||document.documentelement.scrolltop;
	var" percent =" (diffy" + originaltop - document.getelementbyid("bottom").offsettop)/10;
	percent =" percent">0? Math.ceil(percent):Math.floor(percent);
	document.getElementById("bottom").style.top =  document.getElementById("bottom").offsetTop + percent + "px";
}
window.setInterval(heartBeat,3);
